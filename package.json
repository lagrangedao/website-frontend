{
  "name": "lagrange",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build:test": "vue-cli-service build --mode test",
    "build:prod": "vue-cli-service build --mode production",
    "build:testnet": "vue-cli-service build --mode testnet",
    "i18n:report": "vue-cli-service i18n:report --src \"./src/**/*.?(js|vue)\" --locales \"./src/locales/**/*.json\"",
    "serve_t": "set NODE_OPTIONS=\"--openssl-legacy-provider\" & npm run serve\n",
    "build:test_t": "set NODE_OPTIONS=\"--openssl-legacy-provider\" & npm run build:test\n",
    "build:prod_t": "set NODE_OPTIONS=\"--openssl-legacy-provider\" & npm run build:prod\n",
    "serve_u": "export NODE_OPTIONS=\"--openssl-legacy-provider\" && npm run serve\n",
    "build:test_u": "export NODE_OPTIONS=\"--openssl-legacy-provider\" && npm run build:test\n",
    "build:prod_u": "export NODE_OPTIONS=\"--openssl-legacy-provider\" && npm run build:prod\n",
    "build:testnet_t": "set NODE_OPTIONS=\"--openssl-legacy-provider\" & npm run build:testnet\n",
    "build:testnet_u": "export NODE_OPTIONS=\"--openssl-legacy-provider\" && npm run build:testnet\n"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^1.0.0",
    "@kangc/v-md-editor": "^2.3.15",
    "@wagmi/core": "^1.3.8",
    "@web3modal/ethereum": "^2.7.0",
    "@web3modal/html": "^2.7.0",
    "axios": "^0.19.0",
    "babel-polyfill": "^6.26.0",
    "codemirror": "^5.65.13",
    "codemirror-editor-vue3": "^2.3.0",
    "core-js": "^3.6.5",
    "crypto-js": "^4.1.1",
    "echarts": "^5.4.1",
    "element-plus": "^1.3.0-beta.10",
    "element-resize-detector": "^1.2.4",
    "highlight.js": "^11.7.0",
    "moment": "^2.29.4",
    "qs": "^6.10.3",
    "text-loader": "0.0.1",
    "viem": "^1.3.0",
    "vue": "^3.0.0",
    "vue-i18n": "^9.1.9",
    "vue-json-viewer": "^3.0.4",
    "vue-loader": "^17.0.0",
    "vue-router": "^4.0.0-0",
    "vuex": "^4.0.0-0",
    "web3": "^1.7.0"
  },
